                                    <button type="button" data-url='@Url.Action("Buscar", "Naves")' id="btnBuscar" name="Command" value="Buscar" class="btn btn-primary rounded-pill" data-tooltip title="Buscar">
                                        <i class="fa fa-search" aria-hidden="true"></i> Búsqueda
                                    </button>

                                }

                                @if (FrontUser.TienePermiso(RolesPermisos.Usuario_No_Puede_Visualizar_Nuevo_NI))
                                {
                                    <button type="button" id="btnNuevo" name="Command" value="Nuevo" class="btn btn-primary  rounded-pill" data-tooltip title="Nuevo">
                                        <i class="fa fa-file-o" aria-hidden="true"></i> Nuevo
                                    </button>

