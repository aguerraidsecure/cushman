                                    <button type="button" data-url='@Url.Action("Buscar", "Naves")' id="btnBuscarTodo" name="Command" value="Buscar" class="btn btn-primary rounded-pill" data-tooltip title="Búsqueda Básica" data-bs-toggle="modal" data-bs-target="#myModal">
                                        <i class="fa fa-eye" aria-hidden="true"></i> Búsqueda básica
                                    </button>

                                }
